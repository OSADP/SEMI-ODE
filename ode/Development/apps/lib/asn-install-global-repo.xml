<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
  <groupId>asn.1</groupId>
  <artifactId>asn.Install.Lib</artifactId>
  <version>0.0.0</version>
  <description>
   Install Libs into local repository 
  </description>
  <build>
  <plugins>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-install-plugin</artifactId>
            <executions>
                <execution>
                    <id>inst_1</id>
                    <phase>initialize</phase>
                    <goals>
                        <goal>install-file</goal>
                    </goals>
                    <configuration>
							<groupId>asn.1</groupId>
							<artifactId>oss.runtime</artifactId>
							<version>6.2</version>
							<packaging>jar</packaging>
							<file>${project.basedir}\..\..\ASN.1\JavaAPI\semi\lib\osstoed.jar</file>
                    </configuration>
                </execution>
                <execution>
                    <id>inst_2</id>
                    <phase>initialize</phase>
                    <goals>
                        <goal>install-file</goal>
                    </goals>
                    <configuration>
							<groupId>asn.1</groupId>
							<artifactId>semi</artifactId>
							<version>2.2</version>
							<packaging>jar</packaging>
							<file>${project.basedir}\..\..\ASN.1\JavaAPI\semi\target\semi-2.2.jar</file>
                    </configuration>
				</execution>
            </executions>
        </plugin>            
    </plugins>
